{
	"source": "./src",
	"excludes": ["utils"],
	"destination": "./docs",
	"plugins": [{
		"name": "esdoc-standard-plugin",
		"option": {
			"accessor": {
				"access": ["public", "protected"],
				"autoPrivate": true
			}
		}
	}, {
		"name": "esdoc-ecmascript-proposal-plugin",
		"option": {
			"all": true
		}
	}]
}
